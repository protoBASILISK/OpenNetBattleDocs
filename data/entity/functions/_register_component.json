
{
	"version": "2.0",
	"function_name": "register_component",
	"lua_code":
	[
		"local <%=variable_text%>\t-- example <%=variable_text%>",
		"local example_component -- example ScriptedComponent",
		"<%=variable_text%>:register_component( example_component )"
	],
	"description": 
	[
		"Registers a <%=component%> to this <%=class_name%>."
	],
	"parameters": [
		{
			"name": "component",
			"desc": "The <%=component%> you want to register to this <%=class_name%>.",
			"type": "<%=component%>"
		}
	]
}