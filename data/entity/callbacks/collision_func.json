
{
	"version": "2.0",
	"conditional": true,
	"callback_name": "collision_func",
	"lua_code":
	[
		"local <%=variable_text%> -- example <%=variable_text%>",
		"\t<%=variable_text%>.collision_func = function( actor, other )",
		"\tprint( \"collided with another actor\" )",
		"end"
	],
	"description":
	[ 
		"The function that will be called when the <%=class_name%> collides with another <%=entity%>."
	],
	"parameters": [
		{
			"name": "actor",
			"desc": "The <%=class_name%> that will be executing the <%=card_action%>.",
			"type": "<%=class_name%>"
		},
		{
			"name": "other",
			"desc": "The <%=entity%> that collided with this <%=class_name%>.",
			"type": "<%=entity%>"
		}
	],
	"return_value_type": "nil"
}